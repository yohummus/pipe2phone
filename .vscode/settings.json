{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
        "**/.dart_tool": true,
        "**/build": true,
        "**/.venv": true,
        "**/.idea": true
    },
    "dart.lineLength": 120,
    "[dart]": {
        "editor.rulers": [
            120
        ]
    },
    "python.formatting.autopep8Args": [
        "--max-line-length=120"
    ],
    "[python]": {
        "editor.rulers": [
            120
        ]
    },
}